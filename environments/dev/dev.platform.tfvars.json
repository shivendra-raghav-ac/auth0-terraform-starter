{
  "environment": "dev",
  "tenant_friendly_name": "MyCompany DEV",
  "allowed_logout_urls": [
    "http://localhost:3000",
    "https://dev.mycompany.com"
  ],
  "support_email": "support+dev@mycompany.com",
  "support_url": "https://support.mycompany.com",
  "default_redirection_uri": "https://dev.mycompany.com",
  "enabled_locales": [
    "en"
  ],
  "session_lifetime_hours": 168,
  "idle_session_lifetime_hours": 72,
  "non_persistent_cookie": true,
  "branding_logo_url": "https://cdn.mycompany.com/logo.png",
  "primary_color": "#0059d6",
  "background_color": "#000000",
  "universal_login_body_file": "",
  "universal_login_experience": "new",
  "identifier_first": true,
  "webauthn_platform_first_factor": false,
  "enable_prompt_screen_partials": false,
  "prompt_screen_partials_type": "login",
  "prompt_screen_partials": [
    {
      "screen_name": "login",
      "insertion_points": {
        "form_content_start": "<div class=\"banner\">\n  <h2>Welcome!</h2>\n</div>",
        "form_footer_end": "<div class=\"footer\">\n  Â© 2024 MyCompany\n</div>"
      }
    }
  ],
  "allowlist_ips": [],
  "enable_suspicious_ip_throttling": true,
  "suspicious_ip_shields": [
    "admin_notification",
    "block"
  ],
  "suspicious_pre_login_max_attempts": 1,
  "suspicious_pre_login_rate_ms": 86400000,
  "suspicious_pre_user_registration_max_attempts": 50,
  "suspicious_pre_user_registration_rate_ms": 1200,
  "enable_brute_force_protection": true,
  "brute_force_max_attempts": 10,
  "brute_force_mode": "count_per_identifier_and_ip",
  "brute_force_shields": [
    "block",
    "user_notification"
  ],
  "enable_breached_password_detection": false,
  "breached_password_method": "standard",
  "breached_password_shields": [
    "admin_notification",
    "block"
  ],
  "breached_password_pre_user_registration_shields": [
    "block"
  ],
  "breached_password_pre_change_password_shields": [
    "block",
    "admin_notification"
  ],
  "email_from_address": "MyCompany DEV <no-reply@dev.mycompany.com>",
  "email_subject": "Your MyCompany verification code",
  "enable_app": true,
  "app_callbacks": [
    "http://localhost:3000/callback",
    "https://dev.mycompany.com/callback"
  ],
  "app_allowed_logout_urls": [
    "http://localhost:3000",
    "https://dev.mycompany.com"
  ],
  "app_allowed_origins": [
    "http://localhost:3000"
  ],
  "app_web_origins": [
    "http://localhost:3000"
  ],
  "enable_log_stream": false,
  "aws_account_id": "YOUR_AWS_ACCOUNT_ID",
  "aws_region": "us-east-1",
  "log_stream_name": "Auth0 Events - Development",
  "log_stream_status": "active",
  "log_stream_is_priority": false,
  "log_stream_filters": [
    {
      "type": "category",
      "name": "auth.login.fail"
    },
    {
      "type": "category",
      "name": "auth.login.success"
    },
    {
      "type": "category",
      "name": "auth.signup.fail"
    },
    {
      "type": "category",
      "name": "auth.signup.success"
    }
  ],
  "enable_pii_masking": false,
  "pii_method": "hash",
  "pii_algorithm": "xxhash",
  "pii_log_fields": []
}